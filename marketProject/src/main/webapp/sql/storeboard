-- Storeboard 테이블
-- 1) 테이블 생성
create table storeboard (
    seq number not null,                 -- 글번호
    id varchar2(30) not null,            -- 작성자id
    name varchar2(30) not null,          -- 이름 
    city varchar2(30) not null,         -- 시
    gu varchar2(30) not null,            -- 구
    re_ref number not null,              --원글 번호(seq)값과 동일
    re_seq number not null,              --답글의 정렬을 위한 seq변수
    re_lev number not null,              -- 답글의 레벨을 결정하는 변수(답글이 어느 글의 답글인지 분류)
    storeName varchar2(100) not null,       -- 가게이름
    storePlace varchar2(1000) not null,     -- 가게위치
    storeNum varchar2(100) not null,               -- 가게 전화번호
    storeCategory varchar2(20) not null,    -- 가게 카테고리
    storeContent varchar2(4000) not null,   -- 가게소개
    image1 varchar2(200),                   -- 이미지이름
    logtime date default sysdate            -- 작성일
);
-- 2) 테이블 삭제
drop table storeboard purge;
-- 3) 테이블 목록 확인
select * from tab;
--- 검색
select * from storeboard where storePlace like '%서%';
-- 4) 시퀀스 객체 생성
create sequence seq_storeboard nocache nocycle;
-- 5) 시퀀스 객체 삭제
drop sequence seq_storeboard;
-- 6) 정의된 시퀀스 조회
select * from user_storeboard;
commit;


-- 7) 데이터 저장 (insert)
