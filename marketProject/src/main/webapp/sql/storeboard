-- 8) 데이터 검색 (select)
select * from storeboard;
select * from storeboard where seq=1;

-- 9) 데이터 삭제
delete storeboard where seq=23;

-- 10) DB 저장
commit;

-- 11) 인덱스뷰
-- => 행번호(RowNum, 인덱스) 기준으로 데이터를 얻어오는 것
select * from
(select rownum rn, tt.* from
(select * from storeboard order by seq desc) tt)
where rn>=1 and rn<=5;

-- 12) 총데이터갯수 확인
select count(*) as cnt from storeboard;

update storeboard set id='gong', name='고길동',city='서울특별시',gu='강남구',content='아니요',storeName='투썸플레이스', storePlace='서울시 서초구 510', 
storeNum= 025444248,storeCategory ='카페',storeContent='맛있습니다 케이크', image1= 'twosome.jpg' where seq='1';

select seq,city,gu,storeName, storePlace, storeNum, storeCategory,storeContent,image1,
to_char(logtime, 'YYYY.MM.DD') as logtime from (select rownum rn, tt.* from 
(select * from storeboard order by seq desc) tt) where rn>=1 and rn<=5;

select * from storeboard where city='서울특별시' and gu='강남구';

commit;
